<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanci.dao.UserDao">
    <select id="queryUserList" resultType="com.yanci.bean.User">
        select *
        from user
    </select>
    <select id="queryUser" resultType="com.yanci.bean.User">
        select *
        from user
        where id = #{id}
    </select>
</mapper>
